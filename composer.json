{
  "name": "quand/shipmonk-sorted-linkedlist",
  "version": "1.0.0",
  "type": "library",
  "license": "MIT",
  "description": "A small PHP library that implements a SortedLinkedList (keeps values sorted on insert)",
  "keywords": ["sorted", "linked list", "data structure", "list", "collection"],
  "require": {
    "php": "^8.1 || ^8.2 || ^8.3 || ^8.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^12.4",
    "phpstan/phpstan": "^2.1",
    "squizlabs/php_codesniffer": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "ShipMonk\\SortedLinkedList\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ShipMonk\\SortedLinkedList\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Trung Quan Doan",
      "email": "quandt2206@gmail.com"
    }
  ],
  "scripts": {
    "test": "phpunit -c phpunit.xml",
    "stan": "phpstan analyse -l 8 src tests",
    "lint": "phpcs --standard=phpcs.xml src tests",
    "ci": "composer lint && composer stan && composer test"
  }
}
